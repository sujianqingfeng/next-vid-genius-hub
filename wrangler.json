{
  "$schema": "https://raw.githubusercontent.com/cloudflare/workers-sdk/master/wrangler.schema.json",
  "name": "next-vid-genius",
  "main": ".open-next/worker.js",
  "assets": {
    "binding": "ASSETS",
    "directory": ".open-next/assets"
  },
  "compatibility_date": "2024-09-23",
  "compatibility_flags": ["nodejs_compat"],
  "observability": {
    "enabled": true,
    "logs": {
      "enabled": true,
      "head_sampling_rate": 1,
      "invocation_logs": true,
      "persist": true
    }
  },
  "routes": [
    {
      "pattern": "vid-hub.temp-drop-files.store/*",
      "zone_name": "temp-drop-files.store"
    }
  ],
  "services": [
    {
      "binding": "WORKER_SELF_REFERENCE",
      "service": "next-vid-genius"
    }
  ],
  "r2_buckets": [
    {
      "binding": "NEXT_INC_CACHE_R2_BUCKET",
      "bucket_name": "vidgen-render"
    }
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "vidgen_app",
      "database_id": "cb3d8c21-4b76-4bdf-b02d-eea938db23c8",
      "migrations_dir": "drizzle"
    }
  ],
  "vars": {
    "NODE_ENV": "production",
    "NEXT_PUBLIC_APP_URL": "https://vid-hub.temp-drop-files.store",
    "CF_ORCHESTRATOR_URL": "https://media-orchestrator.nieshuanghe.workers.dev",
    "R2_PUBLIC_BASE_URL": ""
  },
  "env": {
    "local": {
      "services": [
        {
          "binding": "WORKER_SELF_REFERENCE",
          "service": "next-vid-genius"
        }
      ],
      "r2_buckets": [
        {
          "binding": "NEXT_INC_CACHE_R2_BUCKET",
          "bucket_name": "vidgen-render"
        }
      ],
      "d1_databases": [
        {
          "binding": "DB",
          "database_name": "vidgen_app",
          "database_id": "cb3d8c21-4b76-4bdf-b02d-eea938db23c8",
          "migrations_dir": "drizzle"
        }
      ],
      "vars": {
        "NODE_ENV": "development",
        "NEXT_PUBLIC_APP_URL": "http://localhost:3000",
        "CF_ORCHESTRATOR_URL": "http://localhost:8787",
        "JOB_CALLBACK_HMAC_SECRET": "replace-with-strong-secret",
        "R2_PUBLIC_BASE_URL": ""
      }
    }
  }
}
