{
  "name": "@app/media-domain",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "description": "Shared media domain constants and types (engine IDs, job statuses, bucket paths).",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "dependencies": {},
