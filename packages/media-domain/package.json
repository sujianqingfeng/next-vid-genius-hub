      "types": "./dist/bucket-paths.d.ts"
    },
    "./job": {
      "import": "./dist/job.js",
      "types": "./dist/job.d.ts"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "typecheck": "tsc -p tsconfig.json --noEmit",
    "clean": "tsdown --clean"
